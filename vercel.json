{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/style.css", "dest": "/style.css" },
    { "src": "/login.js", "dest": "/login.js" },
    { "src": "/machine.js", "dest": "/machine.js" },
    { "src": "/machine", "dest": "/machine.html" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}